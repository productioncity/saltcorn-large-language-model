{
  "name": "@productioncity/saltcorn-large-language-model",
  "version": "0.0.1",
  "description": "Large language models and functionality for Saltcorn",
  "main": "index.js",
  "dependencies": {
    "@google-cloud/aiplatform": "^3.34.0",
    "@google-cloud/vertexai": "^1.9.3",
    "@saltcorn/data": "^0.9.0",
    "googleapis": "^144.0.0",
    "node-fetch": "2.6.9",
    "ollama": "0.5.0",
    "underscore": "1.13.6"
  },
  "devDependencies": {
    "@saltcorn/types": "^1.1.4-beta.9"
  },
  "author": "Tom Nielsen",
  "license": "MIT",
  "repository": "github:productioncity/saltcorn-large-language-model",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@4.9.1",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
